page {
  height: 100vh;
  width: 100vw;
  position: relative;
}
.topTipsExtStyle {
  background-color: #78aa7b;
}
.cu-custom {
  background-color: #78aa7b;
}
.cu-custom .cu-bar .action .icon-jiahao {
  color: #000;
  font-weight: bolder;
}
.cu-custom .cu-bar .action .icon-jiahao:active {
  color: #626263;
}
.cu-custom .cu-bar .content {
  font-size: 32rpx;
  color: #000;
}
.m-priceMaintain-grid {
  background: #FAFAFA;
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
}
.m-priceMaintain-grid .m-grid-head {
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1px solid #78aa7b;
  padding: 20rpx 0;
  margin: 0 20rpx;
}
.m-priceMaintain-grid .m-grid-head p {
  flex: 1;
  text-align: center;
  font-size: 32rpx;
  font-weight: bolder;
}
.m-priceMaintain-grid .m-grid-content {
  position: absolute;
  left: 0rpx;
  right: 0rpx;
  bottom: 0;
  top: 106rpx;
  overflow-y: auto;
  -webkit-overflow-scrolling: touch;
}
.m-priceMaintain-grid .m-grid-content .weui-slideview {
  height: 88rpx !important;
}
.m-priceMaintain-grid .m-grid-content .weui-slideview .m-content-row {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 20rpx;
}
.m-priceMaintain-grid .m-grid-content .weui-slideview .m-content-row p {
  flex: 1;
  text-align: center;
  font-size: 30rpx;
}
.m-priceMaintain-grid .m-grid-content .weui-slideview .m-content-row:active {
  background: rgba(0, 0, 0, 0.1);
}
.halfScreenDialogCell .weui-cell__hd {
  width: 200rpx;
  font-size: 30rpx;
  font-weight: bolder;
}
.halfScreenDialogCell .weui-cell__bd {
  font-size: 30rpx;
}
#footer {
  display: flex;
  justify-content: center;
}
#footer .commonButton {
  padding: 20rpx 80rpx;
  border-radius: 10rpx;
  font-size: 32rpx;
  font-weight: bolder;
}
#footer .commonButton:active {
  opacity: 0.7;
}
#footer .m-footer-cancel {
  background: #F1F1F1;
  color: #49C265;
  margin-right: 40rpx;
}
#footer .m-footer-confirm {
  background: #49C265;
  color: #F1F1F1;
}
