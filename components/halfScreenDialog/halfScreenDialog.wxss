.m-halfScreenDialog-mask {
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  top: 0;
  background-color: rgba(0, 0, 0, 0.65);
  overflow: hidden;
  z-index: 10000;
}
.m-halfScreenDialog-main {
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  height: 70%;
  z-index: 10000;
  background-color: #FFF;
  border-top-left-radius: 30rpx;
  border-top-right-radius: 30rpx;
}
.m-halfScreenDialog-main .m-main-header {
  position: absolute;
  left: 0rpx;
  top: 0rpx;
  right: 0rpx;
  height: 60rpx;
  display: flex;
  padding: 0 20rpx;
  padding-top: 20rpx;
  align-items: center;
}
.m-halfScreenDialog-main .m-main-header .m-header-title {
  position: absolute;
  left: 50%;
  transform: translate(-50%, 0);
  margin-top: 6rpx;
  font-size: 34rpx;
  font-weight: bolder;
}
.m-halfScreenDialog-main .m-main-body {
  position: absolute;
  left: 0;
  right: 0;
  bottom: 120rpx;
  top: 80rpx;
  background: #FAFAFA;
  overflow-y: auto;
  padding-bottom: 20rpx;
}
.m-halfScreenDialog-main .m-main-footer {
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  height: 120rpx;
  background: #FAFAFA;
}
.bottomToTop {
  animation: bottomToTop 0.5s ease 1;
}
.topToBottom {
  animation: topToBottom 0.5s ease 1;
}
@keyframes bottomToTop {
  from {
    height: 0%;
  }
  to {
    height: 70%;
  }
}
@keyframes topToBottom {
  from {
    height: 70%;
  }
  to {
    height: 0%;
  }
}
