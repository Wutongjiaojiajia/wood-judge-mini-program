@import "./styles/iconfont.wxss";

/* 小程序中不支持通配符 */
page,view,text,swiper,swiper-item,image,navigator{
  padding: 0;
  margin: 0;
  box-sizing: border-box;
}
page{
  /* 主题颜色 通过变量实现 */
  --themeColor:#69ac76;
  /* 统一页面字体大小 假设设计稿大小为375px */
  /* 1px=2rpx 14px=28rpx */
  font-size: 28rpx;
  --weixinFontColor:rgba(0,0,0,0.5);
}